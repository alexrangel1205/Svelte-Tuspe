{
  "name": "tuspe",
  "version": "1.0.0",
  "description": "Official website of the company Tuspe Design built in Svelle.",
  "homepage": "https://tuspe.com",
  "bugs": "https://github.com/timoanttila/tuspe/issues",
  "license": "GNU GPL",
  "keywords": [
    "svelte",
    "svelte-framework",
    "svelte-example",
    "svelte-template",
    "rollup",
    "website",
    "company",
    "company-website",
    "scrollto",
    "scroll-events"
  ],
  "author": {
    "name": "Timo Anttila",
    "email": "timo@tuspe.com",
    "url": "https://timoanttila.com"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "run-p watch:routify watch:rollup",
    "watch:rollup": "rollup -c -w",
    "watch:routify": "routify",
    "start": "sirv public --single",
    "css-dev": "node-sass --watch --recursive --output public/css --source-map true --source-map-contents scss"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@sveltech/routify": "^1.5.0-beta.25",
    "autoprefixer": "^9.7.5",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.3.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-livereload": "^1.1.0",
    "rollup-plugin-scss": "^2.1.0",
    "rollup-plugin-svelte": "^5.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "sirv": "^0.4.2",
    "svelte": "^3.20.1",
    "svelte-preprocess": "^3.7.1",
    "svelte-scrollto": "^0.1.1",
    "ygdir": "^1.19.1"
  },
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "sirv-cli": "^0.4.5"
  }
}
